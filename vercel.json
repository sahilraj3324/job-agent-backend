{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "builds": [
        {
            "src": "dist/src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js"
        }
    ]
}